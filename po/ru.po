msgid "Disc:"
msgstr "Диск:"

#: src/interface.c:142
msgid "Album artist:"
msgstr "Исполнитель альбома:"

#: src/interface.c:149
msgid "Album title:"
msgstr "Название альбома:"

#: src/interface.c:156
msgid "Single artist"
msgstr "Один исполнитель"

#: src/interface.c:353
msgid "Preferences"
msgstr "Настройки"

