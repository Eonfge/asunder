msgid "Server: "
msgstr "Server: "

#: src/interface.c:777
msgid "Port: "
msgstr "Port: "

#: src/interface.c:795
msgid "Advanced"
msgstr "Prasunutyja"

#: src/interface.c:866 src/interface.c:902
msgid "Ripping"
msgstr "Ździrańnie"

#: src/interface.c:895
msgid "Total progress"
msgstr "Ahulny prahres"

#: src/interface.c:909
msgid "Encoding"
msgstr "Kadavańnie"

