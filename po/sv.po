#: src/main.c:109 src/interface.c:189
msgid "Rip"
msgstr "Rippa"

#: src/main.c:114
msgid "Track"
msgstr "Spår"

#: src/main.c:121
msgid "Artist"
msgstr "Artist"

#: src/main.c:128
msgid "Title"
msgstr "Titel"

#: src/main.c:133
msgid "Time"
msgstr "Tid"

#: src/main.c:159
msgid ""
"'cdparanoia' was not found in your path. Asunder requires cdparanoia to rip "
"CDs."
msgstr ""
"\"cdparanoia\" hittades inte i dina sökvägar. Asunder kräver cdparanoia för "
"att rippa cd-skivor."

#: src/main.c:369
msgid "<b>Getting disc info from the internet...</b>"
msgstr "<b>Hämtar skivinformation från Internet...</b>"

#: src/callbacks.c:254
msgid "No CD is inserted. Please insert a CD into the CD-ROM drive."
msgstr "Ingen skiva är inmatad. Mata in en skiva i cd-rom-enheten."

#: src/callbacks.c:273 src/callbacks.c:298 src/callbacks.c:323
#, c-format
msgid ""
"%s was not found in your path. Asunder requires it to create %s files. All %"
"s functionality is disabled."
msgstr ""
"%s hittades inte i dina sökvägar. Asunder kräver det för att skapa %s-filer. "
"All %s-funktionalitet har inaktiverats."

#: src/callbacks.c:405
msgid "Select all for ripping"
msgstr "Markera alla för rippning"

#: src/callbacks.c:411
msgid "Deselect all for ripping"
msgstr "Avmarkera alla för rippning"

#: src/interface.c:131
msgid "Disc:"
msgstr "Skiva:"

#: src/interface.c:137
msgid "Album artist:"
msgstr "Albumartist:"

#: src/interface.c:144
msgid "Album title:"
msgstr "Albumtitel:"

#: src/interface.c:151
msgid "Single artist"
msgstr "En artist"

#: src/interface.c:339
msgid "Preferences"
msgstr "Inställningar"

#: src/interface.c:356 src/interface.c:362
msgid "Destination folder"
msgstr "Målmapp"

#: src/interface.c:366
msgid "Create M3U playlist"
msgstr "Skapa M3U-spellista"

#: src/interface.c:374
msgid "CD-ROM device: "
msgstr "Cd-rom-enhet: "

#: src/interface.c:383
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"Standard: /dev/cdrom\n"
"Annat exempel: /dev/hdc\n"
"Annat exempel: /dev/sr0"

#: src/interface.c:387
msgid "Eject disc when finished"
msgstr "Mata ut skivan när den är färdig"

#: src/interface.c:395
msgid "General"
msgstr "Allmänt"

#: src/interface.c:415
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%T - Song title"
msgstr ""
"%A - Artist\n"
"%L - Album\n"
"%N - Spårnummer (2-siffrigt)\n"
"%T - Låttitel"

#: src/interface.c:425
msgid "Album directory: "
msgstr "Albumkatalog: "

#: src/interface.c:432 src/prefs.c:516
msgid "Playlist file: "
msgstr "Spellistfil: "

#: src/interface.c:439 src/prefs.c:528 src/prefs.c:538
msgid "Music file: "
msgstr "Musikfil: "

#: src/interface.c:453
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"Det här är relativt till målmappen (från fliken Allmänt).\n"
"Kan vara blank.\n"
"Standard: %A - %L\n"
"Annat exempel: %A/%L"

#: src/interface.c:465
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"Det här kommer att lagras i albumkatalogen.\n"
"Kan vara blank.\n"
"Standard: %A - %L"

#: src/interface.c:476
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"Det här kommer att lagras i albumkatalogen.\n"
"Får inte vara blank.\n"
"Standard: %A - %T\n"
"Annat exempel: %N - %T"

#: src/interface.c:481
msgid "Filename formats"
msgstr "Filnamnsformat"

#: src/interface.c:486
msgid "Filenames"
msgstr "Filnamn"

#: src/interface.c:506
msgid "WAV (uncompressed)"
msgstr "WAV (okomprimerad)"

#: src/interface.c:523
msgid "Variable bit rate (VBR)"
msgstr "Variabel bitfrekvens (VBR)"

#: src/interface.c:528
msgid "Better quality for the same size."
msgstr "Bättre kvalitet med samma storlek."

#: src/interface.c:534
msgid "Bitrate"
msgstr "Bitfrekvens"

#: src/interface.c:545
msgid ""
"Higher bitrate is better quality but also bigger file. Most people use "
"192Kbps."
msgstr ""
"Högre bitfrekvens ger bättre kvalitet men även en större fil. De flesta "
"använder 192Kbps."

#: src/interface.c:547
msgid "32Kbps"
msgstr "32 Kbps"

#: src/interface.c:551
msgid "MP3 (lossy compression)"
msgstr "MP3 (förstörande komprimering)"

#: src/interface.c:568
msgid "Quality"
msgstr "Kvalitet"

#: src/interface.c:578
msgid "Higher quality means bigger file. Default is 6."
msgstr "Högre kvalitet betyder större fil. Standard är 6."

#: src/interface.c:580
msgid "OGG Vorbis (lossy compression)"
msgstr "OGG Vorbis (förstörande komprimering)"

#: src/interface.c:597
msgid "Compression level"
msgstr "Komprimeringsnivå"

#: src/interface.c:607
msgid "This does not affect the quality. Higher number means smaller file."
msgstr "Det här påverkar inte kvaliteten. Högre tal betyder mindre fil."

#: src/interface.c:609
msgid "FLAC (lossless compression)"
msgstr "FLAC (icke-förstörande komprimering)"

#: src/interface.c:613
msgid "Encode"
msgstr "Kodning"

#: src/interface.c:634
msgid "Get disc info from the internet"
msgstr "Hämta skivinformation från Internet"

#: src/interface.c:642
msgid "Use HTTP proxy to connect to the internet"
msgstr "Använd HTTP-proxy för att ansluta till Internet"

#: src/interface.c:654
msgid "Server: "
msgstr "Server: "

#: src/interface.c:666
msgid "Port: "
msgstr "Port: "

#: src/interface.c:678
msgid "Advanced"
msgstr "Avancerat"

#: src/interface.c:802 src/interface.c:838
msgid "Ripping"
msgstr "Rippa"

#: src/interface.c:831
msgid "Total progress"
msgstr "Totalt förlopp"

#: src/interface.c:845
msgid "Encoding"
msgstr "Kodning"

#: src/interface.c:1042
#, c-format
msgid "%d file created successfully"
msgid_plural "%d files created successfully"
msgstr[0] "%d fil skapades"
msgstr[1] "%d filer skapades"

#: src/interface.c:1051
#, c-format
msgid "There was an error creating %d file"
msgid_plural "There was an error creating %d files"
msgstr[0] "Det uppstod ett fel vid skapandet av %d fil"
msgstr[1] "Det uppstod ett fel vid skapandet av %d filer"

#: src/prefs.c:515 src/prefs.c:527
#, c-format
msgid "Invalid characters in the '%s' field"
msgstr "Ogiltiga tecken i fältet \"%s\""

#: src/prefs.c:537
#, c-format
msgid "'%s' cannot be empty"
msgstr "\"%s\" får inte vara tom"

#: src/prefs.c:552
msgid "Invalid proxy port number"
msgstr "Ogiltigt portnummer för proxy"

#: src/threads.c:128
msgid ""
"No ripping/encoding method selected. Please enable one from the "
"'Preferences' menu."
msgstr ""
"Ingen metod för rippning/kodning har valts. Aktivera en från menyn "
"\"Inställningar\"."

#: src/threads.c:159
msgid ""
"No tracks were selected for ripping/encoding. Please select at least one "
"track."
msgstr "Inga spår har markerats för rippning/kodning. Välj minst ett spår."

#: src/threads.c:616 src/threads.c:618 src/threads.c:622
msgid "Waiting..."
msgstr "Väntar..."

