msgid "Advanced"
msgstr "Pokročilé"

#: src/interface.c:866 src/interface.c:902
msgid "Ripping"
msgstr "Ripovanie"

#: src/interface.c:895
msgid "Total progress"
msgstr "Celkový priebeh"

#: src/interface.c:909
msgid "Encoding"
msgstr "Zakódovanie"

#: src/interface.c:1254
#, c-format
msgid "%d file created successfully"
msgid_plural "%d files created successfully"
msgstr[0] "%d súbor úspešne vytvorený"
msgstr[1] "%d súbory úspešne vytvorené"

#: src/interface.c:1263
#, c-format
msgid "There was an error creating %d file"
msgid_plural "There was an error creating %d files"
msgstr[0] "Chyba pri vytváraní %d súboru"
msgstr[1] "Chyba pri vytváraní %d súborov"

#: src/prefs.c:510 src/prefs.c:522
#, c-format
msgid "Invalid characters in the '%s' field"
msgstr "Neplatné znaky v poli '%s'"

#: src/prefs.c:532
#, c-format
msgid "'%s' cannot be empty"
msgstr "'%s' nesmie byť prázdne"

#: src/prefs.c:547
msgid "Invalid proxy port number"
msgstr "Neplatné číslo portu proxy"

#: src/support.c:47
msgid "Overwrite?"
msgstr "Prepísať?"

#: src/support.c:60
#, c-format
msgid "The file '%s' already exists. Do you want to overwrite it?\n"
msgstr "Súbor '%s' už existuje. Prajete si ho prepísať?\n"

#: src/support.c:66
msgid "Remember the answer for _all the files made from this CD"
msgstr "Zapamätať si odpoveď pre _všetky súbory vytvorené z tohto CD"

#: src/threads.c:138
msgid ""
"No ripping/encoding method selected. Please enable one from the "
"'Preferences' menu."
msgstr ""
"Nebola zvolená metóda ripovania/zakódovania. Prosím, povoľte jednu v menu "
"'Nastavenia'."

#: src/threads.c:169
msgid ""
"No tracks were selected for ripping/encoding. Please select at least one "
"track."
msgstr ""
"Žiadna stopa nebola vybraná pre ripovanie/zakódovanie. Prosím, vyberte aspoň "
"jednu stopu."

#: src/threads.c:696 src/threads.c:698 src/threads.c:702
msgid "Waiting..."
msgstr "Čakám..."
