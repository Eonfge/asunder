msgid "General"
msgstr "Generelt"

#: ../src/interface.c:446
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%Y - Year (4-digit or \"0\")\n"
"%T - Song title"
msgstr ""
"%A - Artist\n"
"%L - Album\n"
"%N - Spornummer (2 siffer)\n"
"%Y - År (4 siffer eller «0»)\n"
"%T - Sportittel"

#: ../src/interface.c:451
#, c-format
msgid "%G - Genre"
msgstr "%G - Sjanger"

