#: src/main.c:109
#: src/interface.c:189
msgid "Rip"
msgstr "吸い出す"

#: src/main.c:114
msgid "Track"
msgstr "トラック"

#: src/main.c:121
msgid "Artist"
msgstr "アーティスト"

#: src/main.c:128
msgid "Title"
msgstr "タイトル"

#: src/main.c:133
msgid "Time"
msgstr "時間"

#: src/main.c:159
msgid "'cdparanoia' was not found in your path. Asunder requires cdparanoia to rip CDs."
msgstr "cdparanoia が見つかりません。CD を吸い出すには Asunder が必要です"

#: src/main.c:377
msgid "<b>Getting disc info from the internet...</b>"
msgstr "<b>ディスクの情報をインターネットから取得</b>"

#: src/callbacks.c:272
msgid "No CD is inserted. Please insert a CD into the CD-ROM drive."
msgstr "CD が挿入されていません。CD を CD ドライブに入れてください。"

#: src/callbacks.c:291
#: src/callbacks.c:316
#: src/callbacks.c:341
#, c-format
msgid "%s was not found in your path. Asunder requires it to create %s files. All %s functionality is disabled."
msgstr "%s が見つかりません。%s を作成するのに必要です。%s の機能を全て無効にします。"

#: src/callbacks.c:423
msgid "Select all for ripping"
msgstr "全てを選択する"

#: src/callbacks.c:429
msgid "Deselect all for ripping"
msgstr "全ての選択を解除"

#: src/interface.c:131
msgid "Disc:"
msgstr "ディスク:"

#: src/interface.c:137
msgid "Album artist:"
msgstr "アーティスト:"

#: src/interface.c:144
msgid "Album title:"
msgstr "アルバムタイトル:"

#: src/interface.c:151
msgid "Single artist"
msgstr "同一のアーティスト"

#: src/interface.c:339
msgid "Preferences"
msgstr "設定"

#: src/interface.c:356
#: src/interface.c:362
msgid "Destination folder"
msgstr "保存先フォルダ"

#: src/interface.c:366
msgid "Create M3U playlist"
msgstr "M3U プレイリストを作る"

#: src/interface.c:374
msgid "CD-ROM device: "
msgstr "CD-ROM デバイス:"

#: src/interface.c:383
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"デフォルト: /dev/cdrom\n"
"例1: /dev/hdc\n"
"例2: /dev/sr0"

#: src/interface.c:387
msgid "Eject disc when finished"
msgstr "完了したらディスクをイジェクト"

#: src/interface.c:395
msgid "General"
msgstr "全般"

#: src/interface.c:415
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%T - Song title"
msgstr ""
"%A - アーティスト\n"
"%L - アルバム\n"
"%N - トラック番号 (2桁)\n"
"%T - 歌のタイトル"

#: src/interface.c:425
msgid "Album directory: "
msgstr "アルバムのディレクトリ:"

#: src/interface.c:432
#: src/prefs.c:516
msgid "Playlist file: "
msgstr "プレイリストファイル:"

#: src/interface.c:439
#: src/prefs.c:528
#: src/prefs.c:538
msgid "Music file: "
msgstr "音楽ファイル:"

#: src/interface.c:453
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"保存先フォルダ (全般タブで場所を指定) に関する設定。\n"
"空白にもできます。\n"
"デフォルト: %A - %L\n"
"他の例: %A/%L"

#: src/interface.c:465
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"アルバムディレクトリに保存されます。\n"
"空白にもできます。\n"
"デフォルト: %A - %L"

#: src/interface.c:476
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"アルバムディレクトリに保存されます。\n"
"空白にもできます。\n"
"デフォルト: %A - %L\n"
"他の例: %N - %T"

#: src/interface.c:481
msgid "Filename formats"
msgstr "ファイル名の形式"

#: src/interface.c:486
msgid "Filenames"
msgstr "ファイル名"

#: src/interface.c:506
msgid "WAV (uncompressed)"
msgstr "WAV (圧縮なし)"

#: src/interface.c:523
msgid "Variable bit rate (VBR)"
msgstr "可変ビットレート (VBR)"

#: src/interface.c:528
msgid "Better quality for the same size."
msgstr "同じサイズでもより良い品質になる"

#: src/interface.c:534
msgid "Bitrate"
msgstr "ビットレート"

#: src/interface.c:545
msgid "Higher bitrate is better quality but also bigger file. Most people use 192Kbps."
msgstr "ビットレートを高くすると音質は良くなるがサイズが大きくなる。多くのユーザは 192Kbps にしている"

#: src/interface.c:547
msgid "32Kbps"
msgstr "32Kbps"

#: src/interface.c:551
msgid "MP3 (lossy compression)"
msgstr "MP3 (ロスあり圧縮)"

#: src/interface.c:568
msgid "Quality"
msgstr "クオリティ"

#: src/interface.c:578
msgid "Higher quality means bigger file. Default is 6."
msgstr "クオリティを上げるとサイズが大きくなる。デフォルトは 6"

#: src/interface.c:580
msgid "OGG Vorbis (lossy compression)"
msgstr "OGG Vorbis (ロスあり圧縮)"

#: src/interface.c:597
msgid "Compression level"
msgstr "圧縮レベル"

#: src/interface.c:607
msgid "This does not affect the quality. Higher number means smaller file."
msgstr "これは品質に影響しない。高い数字にすると小さいファイルになる"

#: src/interface.c:609
msgid "FLAC (lossless compression)"
msgstr "FLAC (ロスなし圧縮)"

#: src/interface.c:613
msgid "Encode"
msgstr "エンコード"

#: src/interface.c:634
msgid "Get disc info from the internet"
msgstr "ディスクの情報をインターネットから取得"

#: src/interface.c:642
msgid "Use HTTP proxy to connect to the internet"
msgstr "インターネット接続に HTTP プロクシを使う"

#: src/interface.c:654
msgid "Server: "
msgstr "サーバ:"

#: src/interface.c:666
msgid "Port: "
msgstr "ポート:"

#: src/interface.c:678
msgid "Advanced"
msgstr "詳細"

#: src/interface.c:802
#: src/interface.c:838
msgid "Ripping"
msgstr "吸い出し"

#: src/interface.c:831
msgid "Total progress"
msgstr "全体の進行"

#: src/interface.c:845
msgid "Encoding"
msgstr "エンコーディング"

#: src/interface.c:1067
#, c-format
msgid "%d file created successfully"
msgid_plural "%d files created successfully"
msgstr[0] "%d 個のファイルを作成しました"

#: src/interface.c:1076
#, c-format
msgid "There was an error creating %d file"
msgid_plural "There was an error creating %d files"
msgstr[0] "%d 個のファイルを作成中にエラー"

#: src/prefs.c:515
#: src/prefs.c:527
#, c-format
msgid "Invalid characters in the '%s' field"
msgstr "'%s' の欄に不正な文字があります"

#: src/prefs.c:537
#, c-format
msgid "'%s' cannot be empty"
msgstr "'%s' は空にできません"

#: src/prefs.c:552
msgid "Invalid proxy port number"
msgstr "不正なプロキシポート番号"

#: src/threads.c:128
msgid "No ripping/encoding method selected. Please enable one from the 'Preferences' menu."
msgstr "吸い出す/エンコーディングする方法を選んでいません。設定メニューで選んでください"

#: src/threads.c:159
msgid "No tracks were selected for ripping/encoding. Please select at least one track."
msgstr "吸い出す/エンコーディングするトラックを選んでいません。最低１つは選んでください"

#: src/threads.c:616
#: src/threads.c:618
#: src/threads.c:622
msgid "Waiting..."
msgstr "待機中..."

