#, c-format
msgid "%dKbps"
msgstr "%dKbps"

#: ../src/callbacks.c:410
msgid "No CD is inserted. Please insert a CD into the CD-ROM drive."
msgstr "CD が挿入されていません。CD を CD-ROM ドライブに入れてください。"

#: ../src/callbacks.c:429 ../src/callbacks.c:454 ../src/callbacks.c:479
#: ../src/callbacks.c:504 ../src/callbacks.c:530 ../src/callbacks.c:555
#: ../src/callbacks.c:580 ../src/callbacks.c:605
#, c-format
msgid ""
"%s was not found in your path. Asunder requires it to create %s files. All "
"%s functionality is disabled."
msgstr ""
"%s が見つかりません。%s を作成するのに必要です。%s の機能を全て無効にします。"

#: ../src/callbacks.c:702
msgid "Select all for ripping"
msgstr "全てを選択する"

#: ../src/callbacks.c:708
msgid "Deselect all for ripping"
msgstr "全ての選択を解除"

#: ../src/interface.c:95
msgid "CDDB Lookup"
msgstr "CDDB 検索"

#: ../src/interface.c:145
msgid "Disc:"
msgstr "ディスク:"

#: ../src/interface.c:151
msgid "Album Artist:"
msgstr "アルバムアーティスト:"

#: ../src/interface.c:158
msgid "Album Title:"
msgstr "アルバムタイトル:"

#: ../src/interface.c:165
msgid "Single Artist"
msgstr "同一のアーティスト"

#: ../src/interface.c:171
msgid "Genre / Year:"
msgstr "ジャンル / 年:"

#: ../src/interface.c:178
msgid "Single Genre"
msgstr "同一のジャンル"

#: ../src/interface.c:370
msgid "Preferences"
msgstr "設定"

#: ../src/interface.c:387 ../src/interface.c:393
msgid "Destination folder"
msgstr "保存先フォルダ"

#: ../src/interface.c:397
msgid "Create M3U playlist"
msgstr "M3U プレイリストを作る"

#: ../src/interface.c:405
msgid "CD-ROM device: "
msgstr "CD-ROM デバイス: "

#: ../src/interface.c:414
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"デフォルト: /dev/cdrom\n"
"例1: /dev/hdc\n"
"例2: /dev/sr0"

#: ../src/interface.c:418
msgid "Eject disc when finished"
msgstr "完了したらディスクをイジェクト"

#: ../src/interface.c:426
msgid "General"
msgstr "全般"

#: ../src/interface.c:446
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%Y - Year (4-digit or \"0\")\n"
"%T - Song title"
msgstr ""
"%A - アーティスト\n"
"%L - アルバム\n"
"%N - トラック番号 (2桁)\n"
"%Y - 年 (4桁または \"0\")\n"
"%T - 歌のタイトル"

#: ../src/interface.c:451
#, c-format
msgid "%G - Genre"
msgstr "%G - ジャンル"

#: ../src/interface.c:466
msgid "Album directory: "
msgstr "アルバムのディレクトリ: "

#: ../src/interface.c:473 ../src/prefs.c:659
msgid "Playlist file: "
msgstr "プレイリストファイル: "

#: ../src/interface.c:480 ../src/prefs.c:671 ../src/prefs.c:681
msgid "Music file: "
msgstr "音楽ファイル: "

#: ../src/interface.c:494
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"保存先フォルダ (全般タブで場所を指定) に関する設定。\n"
"空白にもできます。\n"
"デフォルト: %A - %L\n"
"他の例: %A/%L"

#: ../src/interface.c:506
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"アルバムディレクトリに保存されます。\n"
"空白にもできます。\n"
"デフォルト: %A - %L"

#: ../src/interface.c:517
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"アルバムディレクトリに保存されます。\n"
"空白にもできます。\n"
"デフォルト: %A - %L\n"
"他の例: %N - %T"

#: ../src/interface.c:522
msgid "Filename formats"
msgstr "ファイル名の形式"

#: ../src/interface.c:527
msgid "Filenames"
msgstr "ファイル名"

#. WAV
