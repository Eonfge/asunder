#, c-format
msgid "%dKbps"
msgstr "%dKbps"

#: ../src/callbacks.c:410
msgid "No CD is inserted. Please insert a CD into the CD-ROM drive."
msgstr "Není vloženo CD. Prosím vložte CD-ROM do mechaniky."

#: ../src/callbacks.c:429 ../src/callbacks.c:454 ../src/callbacks.c:479
#: ../src/callbacks.c:504 ../src/callbacks.c:530 ../src/callbacks.c:555
#: ../src/callbacks.c:580 ../src/callbacks.c:605
#, c-format
msgid ""
"%s was not found in your path. Asunder requires it to create %s files. All "
"%s functionality is disabled."
msgstr ""
"%s nebyl nalezen v proměnné $PATH. Asunder ho potřebuje pro vytvoření "
"souborů %s. Všechna funkcionalita %s je zakázána."

#: ../src/callbacks.c:702
msgid "Select all for ripping"
msgstr "Vybrat vše k ripování"

#: ../src/callbacks.c:708
msgid "Deselect all for ripping"
msgstr "Zrušit výběr všeho pro ripování"

#: ../src/interface.c:95
msgid "CDDB Lookup"
msgstr "Vyhledat v databázi CD"

#: ../src/interface.c:145
msgid "Disc:"
msgstr "Disk:"

#: ../src/interface.c:151
msgid "Album Artist:"
msgstr "Umělec alba:"

#: ../src/interface.c:158
msgid "Album Title:"
msgstr "Název alba:"

#: ../src/interface.c:165
msgid "Single Artist"
msgstr "Umělec"

#: ../src/interface.c:171
msgid "Genre / Year:"
msgstr "Styl / Rok"

#: ../src/interface.c:178
msgid "Single Genre"
msgstr "Jednotný žánr"

#: ../src/interface.c:370
msgid "Preferences"
msgstr "Nastavení"

#: ../src/interface.c:387 ../src/interface.c:393
msgid "Destination folder"
msgstr "Cílový adresář"

#: ../src/interface.c:397
msgid "Create M3U playlist"
msgstr "Vytvořit playlist M3U"

#: ../src/interface.c:405
msgid "CD-ROM device: "
msgstr "Zařízení CD-ROM: "

#: ../src/interface.c:414
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"Výchozí: /dev/cdrom\n"
"Další příklad: /dev/hdc\n"
"Další příklad: /dev/sr0"

#: ../src/interface.c:418
msgid "Eject disc when finished"
msgstr "Vysunout disk po dokončení"

#: ../src/interface.c:426
msgid "General"
msgstr "Hlavní"

#: ../src/interface.c:446
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%Y - Year (4-digit or \"0\")\n"
"%T - Song title"
msgstr ""
"%A - Umělec\n"
"%L - Album\n"
"%N - Číslo stopy (dvoumístné)\n"
"%Y - Rok (4-digit nebo \"0\")\n"
"%T - Název stopy"

#: ../src/interface.c:451
#, c-format
msgid "%G - Genre"
msgstr "%G - Styl"

#: ../src/interface.c:466
msgid "Album directory: "
msgstr "Adresář alba: "

#: ../src/interface.c:473 ../src/prefs.c:649
msgid "Playlist file: "
msgstr "Soubor seznamu skladeb: "

#: ../src/interface.c:480 ../src/prefs.c:661 ../src/prefs.c:671
msgid "Music file: "
msgstr "Hudební soubor: "

#: ../src/interface.c:494
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"Toto je relativní k cílovému adresáři (z tabulky Hlavní) \n"
"Může být prázdný.\n"
"Výchozí:%A - %L\n"
"Další příklad: %A/%L"

#: ../src/interface.c:506
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"Toto bude uloženo v adresáři s albem.\n"
"Může být prázdný.\n"
"Výchozí: %A - %L"

#: ../src/interface.c:517
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"Toto bude uloženo v adresáři s albem.\n"
"Nesmí být prázdný.\n"
"Výchozí: %A - %T\n"
"Další příklad: %N - %T"

#: ../src/interface.c:522
msgid "Filename formats"
msgstr "Formáty názvu souboru"

#: ../src/interface.c:527
msgid "Filenames"
msgstr "Názvy souborů"

#. WAV
#: ../src/interface.c:546
msgid "WAV (uncompressed)"
msgstr "WAV (bez komprese)"

#: ../src/interface.c:565
msgid "Variable bit rate (VBR)"
msgstr "Variabilní datový tok (VBR)"

#: ../src/interface.c:573
msgid "Better quality for the same size."
msgstr "Lepší kvalita při stejné velikosti."

#: ../src/interface.c:579 ../src/interface.c:723 ../src/interface.c:808
#: ../src/interface.c:855
msgid "Bitrate"
msgstr "Datový tok"

#: ../src/interface.c:594
msgid ""
"Higher bitrate is better quality but also bigger file. Most people use "
"192Kbps."
msgstr ""
"Vyšší datový tok znamená lepší kvalitu, ale velikost souboru se zvětší. "
"Většina lidí používá 192Kbps."

#: ../src/interface.c:603
msgid "MP3 (lossy compression)"
msgstr "MP3 (ztrátová komprese)"

#: ../src/interface.c:625 ../src/interface.c:966
msgid "Quality"
msgstr "Kvalita"

#: ../src/interface.c:636
msgid "Higher quality means bigger file. Default is 6."
msgstr "Větší kvalita znamená větší soubor. Výchozí je 6."

#: ../src/interface.c:638
msgid "OGG Vorbis (lossy compression)"
msgstr "OGG Vorbis (ztrátová komprese)"

#: ../src/interface.c:660 ../src/interface.c:774 ../src/interface.c:911
msgid "Compression level"
msgstr "Stupeň komprese"

#: ../src/interface.c:671 ../src/interface.c:924
msgid "This does not affect the quality. Higher number means smaller file."
msgstr "Toto nemá vliv na kvalitu. Větší čísla znamenají menší soubor."

#: ../src/interface.c:673
msgid "FLAC (lossless compression)"
msgstr "FLAC (bezztrátová komprese)"

#: ../src/interface.c:692
msgid "More formats"
msgstr "Více formátů"

#: ../src/interface.c:737
msgid ""
"Higher bitrate is better quality but also bigger file. Most people use "
"160Kbps."
msgstr ""
"Vyšší datový tok znamená lepší kvalitu, ale velikost souboru se zvětší. "
"Většina lidí používá 160Kbps."

#: ../src/interface.c:745
msgid "OPUS (lossy compression)"
msgstr "OPUS (ztrátová komprese)"

#: ../src/interface.c:787
msgid ""
"This does not affect the quality. Higher number means smaller file. Default "
"is 1 (recommended)."
msgstr ""
"Toto nemá vliv na kvalitu. Větší čísla znamenají menší soubor. Výchozí "
"hodnota je 1 (doporučená)."

#: ../src/interface.c:793
msgid "Hybrid compression"
msgstr "Hybridní komprese"

#: ../src/interface.c:802
msgid ""
"The format is lossy but a correction file is created for restoring the "
"lossless original."
msgstr ""
"Formát je ztrátový, ale bude vytvořen korekční soubor pro obnovení "
"bezztrátového originálu."

#: ../src/interface.c:871
msgid "Higher bitrate is better quality but also bigger file."
msgstr ""
"Vyšší datový tok znamená lepší kvalitu, ale velikost souboru se zvětší."

#: ../src/interface.c:879
msgid "Musepack (lossy compression)"
msgstr "Musepack (ztrátová komprese)"

#: ../src/interface.c:926
msgid "Monkey's Audio (lossless compression)"
msgstr "Monkey's audio (bezztrátová komprese)"

#. END MONKEY
#: ../src/interface.c:935
msgid "Proprietary encoders"
msgstr "Proprietární enkodery"

#: ../src/interface.c:979
msgid "Higher quality means bigger file. Default is 60."
msgstr "Větší kvalita znamená větší soubor. Výchozí je 60."

#: ../src/interface.c:981
msgid "AAC (lossy compression, Nero encoder)"
msgstr "AAC (ztrátová komprese, enkóder z Nera)"

#. END AAC
#: ../src/interface.c:990
msgid "Encode"
msgstr "Překódovat"

#: ../src/interface.c:1021
msgid "Get disc info from the internet automatically"
msgstr "Získat informaci o disku z internetu automaticky"

#: ../src/interface.c:1029 ../src/interface.c:1074
msgid "Server: "
msgstr "Server: "

#: ../src/interface.c:1039
msgid "The CDDB server to get disc info from (default is freedb.freedb.org)"
msgstr "CDDB server pro získání informací (výchozí je freedb.freedb.org"

#: ../src/interface.c:1045 ../src/interface.c:1087
msgid "Port: "
msgstr "Port: "

#: ../src/interface.c:1055
msgid "The CDDB server port (default is 8880)"
msgstr "Port CDDB serveru (výchozí je 8880)"

#: ../src/interface.c:1061
msgid "Use an HTTP proxy to connect to the internet"
msgstr "Použít HTTP proxy pro připojení k internetu"

#: ../src/interface.c:1105
msgid "Advanced"
msgstr "Pokročilé"

#: ../src/interface.c:1176 ../src/interface.c:1212
msgid "Ripping"
msgstr "Ripování"

#: ../src/interface.c:1205
msgid "Total progress"
msgstr "Celkový průběh"

#: ../src/interface.c:1219
msgid "Encoding"
msgstr "Kódování"

#: ../src/interface.c:1724
msgid ""
"An application to save tracks from an Audio CD \n"
"as WAV, MP3, OGG, FLAC, Opus, Wavpack, Musepack, Monkey's Audio, and/or AAC "
"files."
msgstr ""
"Aplikace pro ukládání skladeb z Audio CD \n"
"jako WAV, MP3, OGG, FLAC, Opus, Wavpack, Musepack, Monkey's Audio, a/nebo "
"AAC files."

#: ../src/interface.c:1764
#, c-format
msgid "%d file created successfully"
msgid_plural "%d files created successfully"
msgstr[0] "%d soubor úspěšně vytvořen"
msgstr[1] "%d soubory úspěšně vytvořeny"

#: ../src/interface.c:1773
#, c-format
msgid "There was an error creating %d file"
msgid_plural "There was an error creating %d files"
msgstr[0] "Chyba při vytváření %d souboru"
msgstr[1] "Chyba při vytváření %d souborů"

#: ../src/prefs.c:648 ../src/prefs.c:660
#, c-format
msgid "Invalid characters in the '%s' field"
msgstr "Neplatné znaky v poli '%s'"

#: ../src/prefs.c:670
#, c-format
msgid "'%s' cannot be empty"
msgstr "'%s' nesmí být prázdné"

#: ../src/prefs.c:685
msgid "Invalid proxy port number"
msgstr "Neplatné číslo portu proxy"

#: ../src/prefs.c:698
msgid "Invalid cddb server port number"
msgstr "Neplatné číslo portu pro CDDB server"

#: ../src/support.c:47
msgid "Overwrite?"
msgstr "Přepsat?"

#: ../src/support.c:60
#, c-format
msgid "The file '%s' already exists. Do you want to overwrite it?\n"
msgstr "Soubor '%s' již existuje. Přejete si ho přepsat?\n"

#: ../src/support.c:66
msgid "Remember the answer for _all the files made from this CD"
msgstr "Zapamatovat si odpověď pro _všechny soubory vytvořené z tohoto CD"

#: ../src/threads.c:183
msgid ""
"No ripping/encoding method selected. Please enable one from the "
"'Preferences' menu."
msgstr ""
"Nebyla zvolena metoda ripování/kódování. Prosím, povolte jednu v menu "
"'Nastavení'."

#: ../src/threads.c:212
msgid ""
"No tracks were selected for ripping/encoding. Please select at least one "
"track."
msgstr ""
"Žádná stopa nebyla vybrána pro ripování/kódování. Prosím, vyberte alespoň "
"jednu stopu."

#: ../src/threads.c:1066 ../src/threads.c:1068 ../src/threads.c:1072
msgid "Waiting..."
msgstr "Čekám..."

