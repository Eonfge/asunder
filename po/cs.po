#: src/main.c:109 src/interface.c:189
msgid "Rip"
msgstr "Ripovat"

#: src/main.c:114
msgid "Track"
msgstr "Stopa"

#: src/main.c:121
msgid "Artist"
msgstr "Umělec"

#: src/main.c:128
msgid "Title"
msgstr "Název"

#: src/main.c:133
msgid "Time"
msgstr "Čas"

#: src/main.c:159
msgid ""
"'cdparanoia' was not found in your path. Asunder requires cdparanoia to rip "
"CDs."
msgstr ""
"'cdparanoia' nebyl nalezen v proměnné $PATH. Asunder potřebuje program "
"cdparanoia, aby mohl ripovat CD."

#: src/main.c:377
msgid "<b>Getting disc info from the internet...</b>"
msgstr "<b>Získávání informací o disku z internetu</b>"

#: src/callbacks.c:272
msgid "No CD is inserted. Please insert a CD into the CD-ROM drive."
msgstr "Není vloženo CD. Prosím vložte CD-ROM do mechaniky."

#: src/callbacks.c:291 src/callbacks.c:316 src/callbacks.c:341
#, c-format
msgid ""
"%s was not found in your path. Asunder requires it to create %s files. All %"
"s functionality is disabled."
msgstr ""
"%s nebyl nalezen v proměnné $PATH. Asunder ho potřebuje pro vytvoření "
"souborů %s. Všechna funkcionalita %s je zakázána."

#: src/callbacks.c:423
msgid "Select all for ripping"
msgstr "Vybrat vše k ripování"

#: src/callbacks.c:429
msgid "Deselect all for ripping"
msgstr "Zrušit výběr všeho pro ripování"

#: src/interface.c:131
msgid "Disc:"
msgstr "Disk:"

#: src/interface.c:137
msgid "Album artist:"
msgstr "Umělec alba:"

#: src/interface.c:144
msgid "Album title:"
msgstr "Název alba:"

#: src/interface.c:151
msgid "Single artist"
msgstr "Jeden umělec"

#: src/interface.c:339
msgid "Preferences"
msgstr "Nastavení"

#: src/interface.c:356 src/interface.c:362
msgid "Destination folder"
msgstr "Cílový adresář"

#: src/interface.c:366
msgid "Create M3U playlist"
msgstr "Vytvořit playlist M3U"

#: src/interface.c:374
msgid "CD-ROM device: "
msgstr "Zařízení CD-ROM: "

#: src/interface.c:383
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"Výchozí: /dev/cdrom\n"
"Další příklad: /dev/hdc\n"
"Další příklad: /dev/sr0"

#: src/interface.c:387
msgid "Eject disc when finished"
msgstr "Vysunout disk po dokončení"

#: src/interface.c:395
msgid "General"
msgstr "Hlavní"

#: src/interface.c:415
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%T - Song title"
msgstr ""
"%A - Umělec\n"
"%L - Album\n"
"%N - Číslo stopy (dvoumístné)\n"
"%T - Název stopy"

#: src/interface.c:425
msgid "Album directory: "
msgstr "Adresář alba: "

#: src/interface.c:432 src/prefs.c:516
msgid "Playlist file: "
msgstr "Soubor seznamu skladeb: "

#: src/interface.c:439 src/prefs.c:528 src/prefs.c:538
msgid "Music file: "
msgstr "Hudební soubor: "

#: src/interface.c:453
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"Toto je relativní k cílovému adresáři (z tabulky Hlavní) \n"
"Může být prázdný.\n"
"Výchozí:%A - %L\n"
"Další příklad: %A/%L"

#: src/interface.c:465
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"Toto bude uloženo v adresáři s albem.\n"
"Může být prázdný.\n"
"Výchozí: %A - %L"

#: src/interface.c:476
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"Toto bude uloženo v adresáři s albem.\n"
"Nesmí být prázdný.\n"
"Výchozí: %A - %T\n"
"Další příklad: %N - %T"

#: src/interface.c:481
msgid "Filename formats"
msgstr "Formáty názvu souboru"

#: src/interface.c:486
msgid "Filenames"
msgstr "Názvy souborů"

#: src/interface.c:506
msgid "WAV (uncompressed)"
msgstr "WAV (bez komprese)"

#: src/interface.c:523
msgid "Variable bit rate (VBR)"
msgstr "Variabilní datový tok (VBR)"

#: src/interface.c:528
msgid "Better quality for the same size."
msgstr "Lepší kvalita při stejné velikosti."

#: src/interface.c:534
msgid "Bitrate"
msgstr "Datový tok"

#: src/interface.c:545
msgid ""
"Higher bitrate is better quality but also bigger file. Most people use "
"192Kbps."
msgstr ""
"Vyšší datový tok znamená lepší kvalitu, ale velikost souboru se zvětší. "
"Většina lidí používá 192Kbps."

#: src/interface.c:547
msgid "32Kbps"
msgstr "32kbps"

#: src/interface.c:551
msgid "MP3 (lossy compression)"
msgstr "MP3 (ztrátová komprese)"

#: src/interface.c:568
msgid "Quality"
msgstr "Kvalita"

#: src/interface.c:578
msgid "Higher quality means bigger file. Default is 6."
msgstr "Větší kvalita znamená větší soubor. Výchozí je 6."

#: src/interface.c:580
msgid "OGG Vorbis (lossy compression)"
msgstr "OGG Vorbis (ztrátová komprese)"

#: src/interface.c:597
msgid "Compression level"
msgstr "Stupeň komprese"

#: src/interface.c:607
msgid "This does not affect the quality. Higher number means smaller file."
msgstr "Toto nemá vliv na kvalitu. Větší čísla znamenají menší soubor."

#: src/interface.c:609
msgid "FLAC (lossless compression)"
msgstr "FLAC (bezztrátová komprese)"

#: src/interface.c:613
msgid "Encode"
msgstr "Překódovat"

#: src/interface.c:634
msgid "Get disc info from the internet"
msgstr "Získat informaci o disku z internetu"

#: src/interface.c:642
msgid "Use HTTP proxy to connect to the internet"
msgstr "Použít HTTP proxy pro připojení k internetu"

#: src/interface.c:654
msgid "Server: "
msgstr "Server: "

#: src/interface.c:666
msgid "Port: "
msgstr "Port: "

#: src/interface.c:678
msgid "Advanced"
msgstr "Pokročilé"

#: src/interface.c:802 src/interface.c:838
msgid "Ripping"
msgstr "Ripování"

#: src/interface.c:831
msgid "Total progress"
msgstr "Celkový průběh"

#: src/interface.c:845
msgid "Encoding"
msgstr "Kódování"

#: src/interface.c:1067
#, fuzzy, c-format
msgid "%d file created successfully"
msgid_plural "%d files created successfully"
msgstr[0] "Všech %d souborů vytvořeno"
msgstr[1] "Všech %d souborů vytvořeno"

#: src/interface.c:1076
#, fuzzy, c-format
