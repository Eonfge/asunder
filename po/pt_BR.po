#: src/interface.c:440
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%Y - Year (4-digit or \"0\")\n"
"%T - Song title"
msgstr ""
"%A - Artista\n"
"%L - Álbum\n"
"%N - Número da faixa (2-dígitos)\n"
"%Y - Ano (4-dígitos ou \"0\")\n"
"%T - Título da música"

#: src/interface.c:450
msgid "Album directory: "
msgstr "Diretório do álbum: "

#: src/interface.c:457
