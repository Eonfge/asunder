msgstr "'%s' não pode estar vazio"

#: src/prefs.c:552
msgid "Invalid proxy port number"
msgstr "Número da porta do proxy inválido"

#: src/threads.c:128
msgid "No ripping/encoding method selected. Please enable one from the 'Preferences' menu."
msgstr "Método de ripar/descodificar não selecionado. Por favor habilite uma no menu \"Preferências\"."

#: src/threads.c:159
msgid "No tracks were selected for ripping/encoding. Please select at least one track."
msgstr "Nenhuma faixa foi selecionada para ripar/descodificar. Por favor selecione ao menos uma faixa."

