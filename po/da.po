#: src/main.c:161
msgid ""
"'cdparanoia' was not found in your path. Asunder requires cdparanoia to rip "
"CDs."
msgstr ""
"'cdparanoia' blev ikke fundet i din sti. Asunder kræver cdparanoia for at "
"kunne udtrække cd'er."

#: src/main.c:400
msgid "<b>Getting disc info from the internet...</b>"
msgstr "<b>Henter cd-information fra internettet...</b>"

#: src/callbacks.c:57 src/callbacks.c:187 src/callbacks.c:215
#: src/callbacks.c:225 src/interface.c:550 src/interface.c:773
#, c-format
msgid "%dKbps"
msgstr "%d Kbps"

#: src/callbacks.c:327
msgid "No CD is inserted. Please insert a CD into the CD-ROM drive."
msgstr "Ingen cd indsat. Indsæt venligst en cd i cd-rom-drevet."

#: src/callbacks.c:346 src/callbacks.c:371 src/callbacks.c:396
#: src/callbacks.c:421 src/callbacks.c:446 src/callbacks.c:471
#: src/callbacks.c:496
#, c-format
msgid ""
"%s was not found in your path. Asunder requires it to create %s files. All %"
"s functionality is disabled."
msgstr ""
"%s blev ikke fundet i din sti. Den er krævet af Asunder for at kunne oprette "
"%s filer. Al %s-funktionalitet er slået fra."

#: src/callbacks.c:583
msgid "Select all for ripping"
msgstr "Vælg alle"

#: src/callbacks.c:589
msgid "Deselect all for ripping"
msgstr "Fravælg alle"

#: src/interface.c:93
msgid "CDDB Lookup"
msgstr "Opslag i CDDB"

#: src/interface.c:136
msgid "Disc:"
msgstr "Cd:"

#: src/interface.c:142
msgid "Album artist:"
msgstr "Kunstner:"

#: src/interface.c:149
msgid "Album title:"
msgstr "Albumtitel:"

#: src/interface.c:156
msgid "Single artist"
msgstr "En kunstner"

#: src/interface.c:333
msgid "Preferences"
msgstr "Indstillinger"

#: src/interface.c:350 src/interface.c:356
msgid "Destination folder"
msgstr "Modtagelsesmappe"

#: src/interface.c:360
msgid "Create M3U playlist"
msgstr "Opret M3U-afspilningsliste"

#: src/interface.c:368
msgid "CD-ROM device: "
msgstr "Cd-rom-enhed: "

#: src/interface.c:377
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"Standard: /dev/cdrom\n"
"Eksempel 1: /dev/hdc\n"
"Eksempel 2: /dev/sr0"

#: src/interface.c:381
msgid "Eject disc when finished"
msgstr "Skub cd'en ud når udtrækket er færdigt"

#: src/interface.c:389
msgid "General"
msgstr "Generelt"

#: src/interface.c:409
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%T - Song title"
msgstr ""
"%A - Kunstner\n"
"%L - Album\n"
"%N - Nr. på albummet (2 cifre)\n"
"%T - Titel"

#: src/interface.c:419
msgid "Album directory: "
msgstr "Albummappe: "

#: src/interface.c:426 src/prefs.c:615
msgid "Playlist file: "
msgstr "Fil for afspilningsliste: "

#: src/interface.c:433 src/prefs.c:627 src/prefs.c:637
msgid "Music file: "
msgstr "Musikfil: "

#: src/interface.c:447
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"Dette er i forhold til modtagelsesmappen (fra det generelle faneblad).\n"
"Må være uudfyldt.\n"
"Standard: %A - L%\n"
"Andet eksempel: %A/%L"

#: src/interface.c:459
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"Denne vil blive gemt i albummappen.\n"
"Kan være uudfyldt.\n"
"Standard: %A - %L"

#: src/interface.c:470
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"Denne vil blive gemt i albummappen.\n"
"Skal være udfyldt.\n"
"Standard: %A - %T\n"
"Andet eksempel: %N - %T"

#: src/interface.c:475
msgid "Filename formats"
msgstr "Filnavnformater"

#: src/interface.c:480
msgid "Filenames"
msgstr "Filnavne"

#. WAV
#: src/interface.c:499
msgid "WAV (uncompressed)"
msgstr "WAV (ukomprimeret)"

#: src/interface.c:518
msgid "Variable bit rate (VBR)"
msgstr "Variabel bitrate (VBR)"

#: src/interface.c:526
msgid "Better quality for the same size."
msgstr "Bedre kvalitet med den samme størrelse."

#: src/interface.c:532 src/interface.c:708 src/interface.c:755
msgid "Bitrate"
msgstr "Bitrate"

#: src/interface.c:547
msgid ""
"Higher bitrate is better quality but also bigger file. Most people use "
"192Kbps."
msgstr ""
"Højere bitrate giver bedre kvalitet men også større filer. De fleste "
"bruger 192 Kbps."

#: src/interface.c:556
msgid "MP3 (lossy compression)"
msgstr "MP3 (kompression med kvalitetstab)"

#: src/interface.c:578 src/interface.c:866
msgid "Quality"
msgstr "Kvalitet"

#: src/interface.c:589
msgid "Higher quality means bigger file. Default is 6."
msgstr "Højere kvalitet betyder større filer. Standard er 6."

#: src/interface.c:591
msgid "OGG Vorbis (lossy compression)"
msgstr "OGG Vorbis (kompression med kvalitetstab)"

#: src/interface.c:613 src/interface.c:674 src/interface.c:811
msgid "Compression level"
msgstr "Kompressionsniveau"

#: src/interface.c:624 src/interface.c:824
msgid "This does not affect the quality. Higher number means smaller file."
msgstr "Dette påvirker ikke kvaliteten. Højere tal betyder mindre filer."

#: src/interface.c:626
msgid "FLAC (lossless compression)"
msgstr "FLAC (kompression uden kvalitetstab)"

#: src/interface.c:645
msgid "More formats"
msgstr "Flere formater"

#: src/interface.c:687
msgid ""
"This does not affect the quality. Higher number means smaller file. Default "
"is 1 (recommended)."
msgstr ""
"Dette påvirker ikke kvaliteten. Højere tal betyder mindre filer. Standard er "
"1 (anbefalet)."

#: src/interface.c:693
msgid "Hybrid compression"
msgstr "Blandet kompression"

#: src/interface.c:702
msgid ""
"The format is lossy but a correction file is created for restoring the "
"lossless original."
msgstr ""
"Dette format har kvalitetstab men en korrektionsfil oprettes så filen "
"uden kvalitetstab kan genskabes."

#: src/interface.c:771
msgid "Higher bitrate is better quality but also bigger file."
msgstr "Højere bitrate medfører bedre kvalitet men også større filer."

#: src/interface.c:779
msgid "Musepack (lossy compression)"
msgstr "Musepack (kompression med kvalitetstab)"

#: src/interface.c:826
msgid "Monkey's Audio (lossless compression)"
msgstr "Monkeys Audio (kompression uden kvalitetstab)"

#. END MONKEY
#: src/interface.c:835
msgid "Proprietary encoders"
msgstr "Propritær kodning"

#: src/interface.c:879
msgid "Higher quality means bigger file. Default is 60."
msgstr "Højere kvalitet medfører større filer. Standard er 60."

#: src/interface.c:881
msgid "AAC (lossy compression, Nero encoder)"
msgstr "AAC (kompression med tab, Nerokodning)"

#. END AAC
#: src/interface.c:890
msgid "Encode"
msgstr "Konverter"

#: src/interface.c:921
msgid "Get disc info from the internet automatically"
msgstr "Hent automatisk cd-info fra internettet"

#: src/interface.c:929 src/interface.c:974
msgid "Server: "
msgstr "Server: "

#: src/interface.c:939
msgid "The CDDB server to get disc info from (default is freedb.org)"
msgstr "CDDB-serveren som cd-infoen skal hentes fra (standard er freedb.org)"

#: src/interface.c:945 src/interface.c:987
msgid "Port: "
msgstr "Port: "

#: src/interface.c:955
msgid "The CDDB server port (default is 888)"
msgstr "CDDB-serverporten (standard er 888)"

#: src/interface.c:961
msgid "Use an HTTP proxy to connect to the internet"
msgstr "Brug en HTTP-proxy til internetforbindelsen"

#: src/interface.c:1005
msgid "Advanced"
msgstr "Avanceret"

#: src/interface.c:1076 src/interface.c:1112
msgid "Ripping"
msgstr "Udtrækning"

#: src/interface.c:1105
msgid "Total progress"
msgstr "Samlet fremskridt"

#: src/interface.c:1119
msgid "Encoding"
msgstr "Konvertering"

#: src/interface.c:1507
#, c-format
msgid "%d file created successfully"
msgid_plural "%d files created successfully"
msgstr[0] "%d fil oprettet"
msgstr[1] "%d filer oprettet"

#: src/interface.c:1516
#, c-format
msgid "There was an error creating %d file"
msgid_plural "There was an error creating %d files"
msgstr[0] "En fejl opstod under oprettelse af %d fil"
msgstr[1] "En fejl opstod under oprettelse af %d filer"

#: src/prefs.c:614 src/prefs.c:626
#, c-format
msgid "Invalid characters in the '%s' field"
msgstr "Ugyldige tegn i feltet '%s'"

#: src/prefs.c:636
#, c-format
msgid "'%s' cannot be empty"
msgstr "'%s' kan ikke være tom"

#: src/prefs.c:651
msgid "Invalid proxy port number"
msgstr "Ugyldig portnummer på proxyen"

#: src/prefs.c:664
msgid "Invalid cddb server port number"
msgstr "Ugyldigt cddb-serverportnummer"

#: src/support.c:47
msgid "Overwrite?"
msgstr "Overskriv?"

#: src/support.c:60
#, c-format
msgid "The file '%s' already exists. Do you want to overwrite it?\n"
msgstr "Filen '%s' eksisterer allerede. Ønsker du at overskrive den?\n"

#: src/support.c:66
msgid "Remember the answer for _all the files made from this CD"
msgstr "Husk svaret for _alle filerne oprettet fra denne cd"

#: src/threads.c:159
msgid ""
"No ripping/encoding method selected. Please enable one from the "
"'Preferences' menu."
msgstr ""
"Ingen udtræks-/konverteringsmetode valgt. Slå venligt en metode "
"til via 'Indstillinger'."

#: src/threads.c:190
msgid ""
"No tracks were selected for ripping/encoding. Please select at least one "
"track."
msgstr ""
"Ingen numre valgt for udtrækning/konvertering. Vælg venligst mindst et "
"nummer."

#: src/threads.c:877 src/threads.c:879 src/threads.c:883
msgid "Waiting..."
msgstr "Afventer..."
