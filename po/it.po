msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"Verrà salvato nella directory album.\n"
"Può essere vuoto.\n"
"Predefinito: %A - %L"

#: src/interface.c:490
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"Verrà salvato nella directory album.\n"
"Non può essere vuoto.\n"
"Predefinito: %A - %T\n"
"Altro esempio: %N - %T"

