#, c-format
msgid "%dKbps"
msgstr "%dKbps"

#: ../src/callbacks.c:410
msgid "No CD is inserted. Please insert a CD into the CD-ROM drive."
msgstr "Nema CD-a. Molim, ubacite CD u optički uređaj."

#: ../src/callbacks.c:429 ../src/callbacks.c:454 ../src/callbacks.c:479
#: ../src/callbacks.c:504 ../src/callbacks.c:530 ../src/callbacks.c:555
#: ../src/callbacks.c:580 ../src/callbacks.c:605
#, c-format
msgid ""
"%s was not found in your path. Asunder requires it to create %s files. All "
"%s functionality is disabled."
msgstr ""
"%s nije pronađen na sustavu. Asunderu je potreban kako bi stvorio %s datoteke. Sva"
"%s funkcionalnost je onemogućena."

#: ../src/callbacks.c:702
msgid "Select all for ripping"
msgstr "Označi sve za ripanje"

#: ../src/callbacks.c:708
msgid "Deselect all for ripping"
msgstr "Odznači sve za ripanje"

#: ../src/interface.c:95
msgid "CDDB Lookup"
msgstr "CDDB pretraga"

#: ../src/interface.c:145
msgid "Disc:"
msgstr "Disk"

#: ../src/interface.c:151
msgid "Album Artist:"
msgstr "Izvođač albuma:"

#: ../src/interface.c:158
msgid "Album Title:"
msgstr "Naslov albuma:"

#: ../src/interface.c:165
msgid "Single Artist"
msgstr "Jedan izvođač"

#: ../src/interface.c:171
msgid "Genre / Year:"
msgstr "Vrsta / Godina:"

#: ../src/interface.c:178
msgid "Single Genre"
msgstr "Jedna vrsta"

#: ../src/interface.c:370
msgid "Preferences"
msgstr "Postavke"

#: ../src/interface.c:387 ../src/interface.c:393
msgid "Destination folder"
msgstr "Odredišni folder"

#: ../src/interface.c:397
msgid "Create M3U playlist"
msgstr "Napravi M3U listu"

#: ../src/interface.c:405
msgid "CD-ROM device: "
msgstr "Optički uređaj: "

#: ../src/interface.c:414
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"Uobičajeno: /dev/cdrom\n"
"Drugi primjer: /dev/hdc\n"
"Drugi primjer: /dev/sr0"

#: ../src/interface.c:418
msgid "Eject disc when finished"
msgstr "Izbaci disk po završetku"

#: ../src/interface.c:426
msgid "General"
msgstr "Općenito"

#: ../src/interface.c:446
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%Y - Year (4-digit or \"0\")\n"
"%T - Song title"
msgstr ""
"%A - Izvođač\n"
"%L - Album\n"
"%N - Redni broj (dvoznamenkasti)\n"
"%Y - Godina (četveroznamenkasta ili \"0\")\n"
"%T - Naslov pjesme"

#: ../src/interface.c:451
#, c-format
msgid "%G - Genre"
msgstr "%G - Vrsta"

#: ../src/interface.c:466
msgid "Album directory: "
msgstr "Direktorij albuma: "

#: ../src/interface.c:473 ../src/prefs.c:659
msgid "Playlist file: "
msgstr "Datoteka liste: "

#: ../src/interface.c:480 ../src/prefs.c:671 ../src/prefs.c:681
msgid "Music file: "
msgstr "Glazbena datoteka: "

#: ../src/interface.c:494
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"Ovo je putanja do odredišnog foldera (prema karitci Općenito).\n"
"Može biti prazno.\n"
"Uobičajeno: %A - %L\n"
"Drugi primjer: %A/%L"

#: ../src/interface.c:506
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"Ovo će biti pohranjeno u direktoriju albuma.\n"
"Može biti prazno.\n"
"Uobičajeno: %A - %L"

#: ../src/interface.c:517
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"Ovo će biti pohranjeno u direktoriju albuma.\n"
"Ne može biti prazno.\n"
"Uobičajeno: %A - %T\n"
"Drugi primjer: %N - %T"

#: ../src/interface.c:522
msgid "Filename formats"
msgstr "Format imena datoteka"

#: ../src/interface.c:527
msgid "Filenames"
msgstr "Imena datoteka"

