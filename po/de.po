msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%Y - Year (4-digit or \"0\")\n"
"%T - Song title"
msgstr ""
"%A - Interpret\n"
"%L - Album\n"
"%N - Titelnr. (2-stellig)\n"
"%Y - Jahr (4-stellig oder \"0\")\n"
"%T - Titel"

