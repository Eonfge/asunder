#: src/main.c:113
#: src/interface.c:203
msgid "Rip"
msgstr "Përftoje"

#: src/main.c:118
msgid "Track"
msgstr "Gjurmë"

#: src/main.c:125
msgid "Artist"
msgstr "Artist"

#: src/main.c:132
msgid "Title"
msgstr "Titull"

#: src/main.c:137
msgid "Time"
msgstr "Kohë"

#: src/main.c:163
msgid "'cdparanoia' was not found in your path. Asunder requires cdparanoia to rip CDs."
msgstr "'cdparanoia' nuk u gjet te shtegu juaj. Asunder-i lyp cdparanoia-an për përftim CD-sh."

#: src/main.c:434
msgid "<b>Getting disc info from the internet...</b>"
msgstr "<b>Po marr të dhëna disku prej interneti...</b>"

#: src/callbacks.c:283
msgid "No CD is inserted. Please insert a CD into the CD-ROM drive."
msgstr "Nuk është futur CD. Ju lutem fusni një CD te pajisja CD-ROM."

#: src/callbacks.c:302
#: src/callbacks.c:327
#: src/callbacks.c:352
#, c-format
msgid "%s was not found in your path. Asunder requires it to create %s files. All %s functionality is disabled."
msgstr "%s nuk u gjet në shtegun tuaj. Asunder-i e ka të nevojshme që të krijojë kartela %s. Gjithë ana %s është çaktivizuar."

#: src/callbacks.c:439
msgid "Select all for ripping"
msgstr "Përzgjidhini të tëra për përftim"

#: src/callbacks.c:445
msgid "Deselect all for ripping"
msgstr "Shpërzgjidhini të tëra për përftim"

#: src/interface.c:93
msgid "CDDB Lookup"
msgstr "Kërkim në CDDB"

#: src/interface.c:136
msgid "Disc:"
msgstr "Disk:"

#: src/interface.c:142
msgid "Album artist:"
msgstr "Artist albumi:"

#: src/interface.c:149
msgid "Album title:"
msgstr "Titull albumi:"

#: src/interface.c:156
msgid "Single artist"
msgstr "Artist pjese"

#: src/interface.c:353
msgid "Preferences"
msgstr "Parapëlqime"

#: src/interface.c:370
#: src/interface.c:376
msgid "Destination folder"
msgstr "Dosje vendmbërritje"

#: src/interface.c:380
msgid "Create M3U playlist"
msgstr "Krijo luajlistë M3U"

#: src/interface.c:388
msgid "CD-ROM device: "
msgstr "Pajisje CD-ROM: "

#: src/interface.c:397
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"Parazgjedhje: /dev/cdrom\n"
"Shembull tjetër: /dev/hdc\n"
"Shembull tjetër: /dev/sr0"

#: src/interface.c:401
msgid "Eject disc when finished"
msgstr "Nxirre diskun kur përfundohet"

#: src/interface.c:409
msgid "General"
msgstr "Të përgjithshme"

#: src/interface.c:429
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%T - Song title"
msgstr ""
"%A - Artist\n"
"%L - Album\n"
"%N - Numër gjurme (me 2 shifra)\n"
"%T - Titull pjese"

#: src/interface.c:439
msgid "Album directory: "
msgstr "Drejtori albumi: "

#: src/interface.c:446
#: src/prefs.c:516
msgid "Playlist file: "
msgstr "Kartelë luajlistë: "

#: src/interface.c:453
#: src/prefs.c:528
#: src/prefs.c:538
msgid "Music file: "
msgstr "Kartelë muzikore: "

#: src/interface.c:467
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"Kjo ka të bëjë me dosjen përfundim (prej skedës Të përgjithshme).\n"
"Mund të jetë bosh.\n"
"Parazgjedhje: %A - %L\n"
"Shembull tjetër: %A/%L"

#: src/interface.c:479
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"Kjo do të ruhet te drejtoria e albumit.\n"
"Mund të jetë bosh.\n"
"Parazgjedhje: %A - %L"

#: src/interface.c:490
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"Kjo do të ruhet te drejtoria e albumit.\n"
"Nuk mund të jetë bosh.\n"
"Parazgjedhje: %A - %T\n"
"Shembull tjetër: %N - %T"

#: src/interface.c:495
msgid "Filename formats"
msgstr "Formate emrash kartelash:"

#: src/interface.c:500
msgid "Filenames"
msgstr "Emra kartelash"

#: src/interface.c:520
msgid "WAV (uncompressed)"
msgstr "WAV (e pangjeshur)"

#: src/interface.c:537
msgid "Variable bit rate (VBR)"
msgstr "Bitrate i ndryshueshëm (VBR)"

#: src/interface.c:542
msgid "Better quality for the same size."
msgstr "Cilësi më e mirë për të njëjtën madhësi."

#: src/interface.c:548
msgid "Bitrate"
msgstr "Bitrate"

#: src/interface.c:559
msgid "Higher bitrate is better quality but also bigger file. Most people use 192Kbps."
msgstr "Bitrate më i lartë do të thotë cilësi më e mirë por gjithashtu një kartelë më e madhe. Shumica e njerëzve përdorin 192Kbps."

#: src/interface.c:561
msgid "32Kbps"
msgstr "32Kbps"

#: src/interface.c:565
msgid "MP3 (lossy compression)"
msgstr "MP3 (ngjeshje me humbje)"

#: src/interface.c:582
msgid "Quality"
msgstr "Cilësi"

#: src/interface.c:592
msgid "Higher quality means bigger file. Default is 6."
msgstr "Cilësi më e lartë do të thotë kartelë më e madhe. Parazgjedhja është 6."

#: src/interface.c:594
msgid "OGG Vorbis (lossy compression)"
msgstr "OGG Vorbis (ngjeshje me humbje)"

#: src/interface.c:611
msgid "Compression level"
msgstr "Nivel ngjeshjeje"

#: src/interface.c:621
msgid "This does not affect the quality. Higher number means smaller file."
msgstr "Kjo nuk prek cilësinë. Numër më i madh do të thotë kartelë më e vogël."

#: src/interface.c:623
msgid "FLAC (lossless compression)"
msgstr "FLAC (Ngjeshje pa humbje)"

#: src/interface.c:627
msgid "Encode"
msgstr "Kodo"

#: src/interface.c:648
msgid "Get disc info from the internet automatically"
msgstr "Merr vetvetiu të dhëna disku prej internetit"

#: src/interface.c:656
msgid "Use HTTP proxy to connect to the internet"
msgstr "Përdor ndërmjetës HTTP për lidhje në internet"

#: src/interface.c:668
msgid "Server: "
msgstr "Shërbyes"

#: src/interface.c:680
msgid "Port: "
msgstr "Portë:"

#: src/interface.c:692
msgid "Advanced"
msgstr "Të mëtejshme"

#: src/interface.c:816
#: src/interface.c:852
msgid "Ripping"
msgstr "Përftim"

#: src/interface.c:845
msgid "Total progress"
msgstr "Ecuri gjithsej"

#: src/interface.c:859
msgid "Encoding"
msgstr "Kodim"

#: src/interface.c:1111
#, c-format
msgid "%d file created successfully"
msgid_plural "%d files created successfully"
msgstr[0] "u krijua me sukses %d kartelë "
msgstr[1] "u krijuan me sukses %d kartela"

#: src/interface.c:1120
#, c-format
msgid "There was an error creating %d file"
msgid_plural "There was an error creating %d files"
msgstr[0] "Pati gabim gjatë krijimit të %d kartele"
msgstr[1] "Pati gabim gjatë krijimit të %d kartelave"

#: src/prefs.c:515
#: src/prefs.c:527
#, c-format
msgid "Invalid characters in the '%s' field"
msgstr "Shenja të pavlefshme te fusha '%s'"

#: src/prefs.c:537
#, c-format
msgid "'%s' cannot be empty"
msgstr "'%s' nuk mund të jetë bosh"

#: src/prefs.c:552
msgid "Invalid proxy port number"
msgstr "Numër i pavlefshëm porte ndërmjetësi"

#: src/support.c:47
msgid "Overwrite?"
msgstr "Ta mbishkruaj?"

#: src/support.c:60
#, c-format
msgid "The file '%s' already exists. Do you want to overwrite it?\n"
msgstr "Ka tashmë një kartelë '%s'. Dni të mbishkruhet ajo?\n"

#: src/support.c:66
msgid "Remember the answer for _all the files made from this CD"
msgstr "Mbaje mend përgjigjen për _tërë kartelat e krijuara prej këtij CD-je"

#: src/threads.c:142
msgid "No ripping/encoding method selected. Please enable one from the 'Preferences' menu."
msgstr "Nuk është përzgjedhur metodë përftimi/kodimi. Ju lutem aktivizoni një të tillë prej menusë 'Parapëlqime'."

#: src/threads.c:173
msgid "No tracks were selected for ripping/encoding. Please select at least one track."
msgstr "Nuk janë përzgjedhur gjurmë për përftim/kodim. Ju lutem përzgjidhni të paktën një gjurmë."

