msgid "Preferences"
msgstr "Preferencje"

#: src/interface.c:370 src/interface.c:376
msgid "Destination folder"
msgstr "Katalog docelowy"

#: src/interface.c:380
msgid "Create M3U playlist"
msgstr "Utwórz playlistę M3U"

#: src/interface.c:388
msgid "CD-ROM device: "
msgstr "Urządzenie CD-ROM: "

#: src/interface.c:397
msgid ""
"Default: /dev/cdrom\n"
"Other example: /dev/hdc\n"
"Other example: /dev/sr0"
msgstr ""
"Domyślnie: /dev/cdrom\n"
"Inny przykład: /dev/hdc\n"
"Inny przykład: /dev/sr0"

#: src/interface.c:401
msgid "Eject disc when finished"
msgstr "Wysuń dysk po zakończeniu"

#: src/interface.c:409
msgid "General"
msgstr "Ogólne"

#: src/interface.c:429
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%T - Song title"
msgstr ""
"%A - Artysta\n"
"%L - Album\n"
"%N - Numer ścieżki (2-znaki)\n"
"%T - Tytuł utworu"

#: src/interface.c:439
msgid "Album directory: "
msgstr "Katalog albumu: "

#: src/interface.c:446 src/prefs.c:516
msgid "Playlist file: "
msgstr "Plik playlisty: "

#: src/interface.c:453 src/prefs.c:528 src/prefs.c:538
msgid "Music file: "
msgstr "Plik muzyczny: "

#: src/interface.c:467
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"Zależy od położenia katalogu docelowego (zakładka Ogólne).\n"
"Może być puste.\n"
"Domyślnie: %A - %L\n"
"Inny przykład: %A/%L"

#: src/interface.c:479
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"Zostanie umieszczony w katalogu albumu.\n"
"Może być puste.\n"
"Domyślnie: %A - %L"

#: src/interface.c:490
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"Zostanie umieszczony w katalogu albumu.\n"
"Nie może być puste.\n"
"Domyślnie: %A - %T\n"
"Inny przykład: %N - %T"

#: src/interface.c:495
msgid "Filename formats"
msgstr "Format nazwy pliku"

#: src/interface.c:500
msgid "Filenames"
msgstr "Nazwy plików"

#: src/interface.c:520
msgid "WAV (uncompressed)"
msgstr "WAV (nieskompresowany)"

#: src/interface.c:537
msgid "Variable bit rate (VBR)"
msgstr "Zmienny bitrate (VBR)"

#: src/interface.c:542
msgid "Better quality for the same size."
msgstr "Lepsza jakość przy zachowaniu rozmiaru pliku."

#: src/interface.c:548
msgid "Bitrate"
msgstr "Bitrate"

#: src/interface.c:559
msgid ""
"Higher bitrate is better quality but also bigger file. Most people use "
"192Kbps."
msgstr ""
"Wyższa wartość bitrate oznacza lepszą jakość przy większym rozmiarze pliku."
"Większość ludzi używa wartości 192Kbps."

#: src/interface.c:561
msgid "32Kbps"
msgstr "32Kbps"

#: src/interface.c:565
msgid "MP3 (lossy compression)"
msgstr "MP3 (kompresja stratna)"

#: src/interface.c:582
msgid "Quality"
msgstr "Jakość"

#: src/interface.c:592
msgid "Higher quality means bigger file. Default is 6."
msgstr "Wyższa jakość oznacza większy plik. Domyślna wartość to 6."

#: src/interface.c:594
msgid "OGG Vorbis (lossy compression)"
msgstr "OGG Vorbis (kompresja stratna)"

#: src/interface.c:611
msgid "Compression level"
msgstr "Poziom kompresji"

#: src/interface.c:621
msgid "This does not affect the quality. Higher number means smaller file."
msgstr "Nie wpływa na jakość. Większa wartość oznacza mniejszy rozmiar pliku."

#: src/interface.c:623
msgid "FLAC (lossless compression)"
msgstr "FLAC (kompresja bezstratna)"

#: src/interface.c:627
msgid "Encode"
msgstr "Koduj"

#: src/interface.c:648
#, fuzzy
msgid "Get disc info from the internet automatically"
msgstr "Pobierz informacje o dysku z internetu"

#: src/interface.c:656
msgid "Use HTTP proxy to connect to the internet"
msgstr "Używaj serwera proxy do łączenia z internetem"

#: src/interface.c:668
msgid "Server: "
msgstr "Serwer: "

#: src/interface.c:680
msgid "Port: "
msgstr "Port: "

#: src/interface.c:692
msgid "Advanced"
msgstr "Zaawansowane"

#: src/interface.c:816 src/interface.c:852
msgid "Ripping"
msgstr "Zgrywanie"

#: src/interface.c:845
msgid "Total progress"
msgstr "Całkowity postęp"

#: src/interface.c:859
msgid "Encoding"
msgstr "Kodowanie"

